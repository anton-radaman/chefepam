#!/usr/bin/python
import os
from datetime import datetime

print("Content-type: text/html\r\n\r\n")
print("<font size=+10>Environment</font><br>")
date = datetime.now().strftime('%Y-%m-%d %H:%M:%S')
param = 'HTTP_VDI'
#print "<b>Date_Time</b>:{1}<br>".format(date, os.environ[param])
line = '{1}, Vdi_name = {0}, State = 1\n'.format(os.environ[param], date)
with open('/var/www/html/log-request.log', 'a+') as log:
    log.write(line)
